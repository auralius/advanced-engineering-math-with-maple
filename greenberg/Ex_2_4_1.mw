<Group hide-input="false" labelreference="L19" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">separation_of_variables:=proc(eqX, eqY, x0,y0)
  local eq1, eq2, eq3, eq4, eq5, eq6, eq7, eq8, eq9;<Equation executable="true" style="2D Input" input-equation="" display="LUklbXJvd0c2Iy9JK21vZHVsZW5hbWVHNiJJLFR5cGVzZXR0aW5nR0koX3N5c2xpYkdGJzYlLUkjbWlHRiQ2I1EhRicvJStleGVjdXRhYmxlR1EmZmFsc2VGJy8lLG1hdGh2YXJpYW50R1Enbm9ybWFsRic=">JSFH</Equation>
  eq1:=diff(y(x),x)=eqX*eqY<Equation executable="true" style="2D Input" input-equation="" display="LUklbXJvd0c2Iy9JK21vZHVsZW5hbWVHNiJJLFR5cGVzZXR0aW5nR0koX3N5c2xpYkdGJzYlLUkjbWlHRiQ2I1EhRicvJStleGVjdXRhYmxlR1EmZmFsc2VGJy8lLG1hdGh2YXJpYW50R1Enbm9ybWFsRic=">JSFH</Equation>;

  printf(&quot;An ODE:&quot;);
  print(eq1);

  printf(&quot;with:&quot;);
  print('X(x)' = eqX);
  print('Y(y)' = eqY);

  printf(&quot;Therefore&quot;);
  print('Int(1/Y(y),y)'='Int(X(x),x)');
  eq1:=int(eqX,x);
  eq2:=int(1/eqY,y);
  eq3:=eq2=eq1+C;
  eq4:=y(x)=solve(eq3,y);

  print(Int(1/eqY,y)=Int(eqX,x)); 
  print(eq3);

  printf(&quot;General solution:&quot;);
  print(sort(eq4,x));

  printf(&quot;Particular solution for the given IC:&quot;);
  print('y(x0)=y0');
  
  printf(&quot;Thus:&quot;);
  eq5:=eval(eq4, x=x0);
  eq6:=y0=rhs(eq5);
  eq7:=C=solve(eq6,C);

  print(eq5);
  print(eq6);
  print(eq7);

  if solve(eq6,C)=NULL then 
    printf(&quot;Fail to calculate C, need to be creative here.&quot;);
  else
    printf(&quot;Therefore, the particular solution is:&quot;);
    eq8:=subs(eq7,eq4);
    print(simplify(eq8));

    printf(&quot;Verify the intial condition:&quot;);
    eq9:=simplify(eval(eq8, x=x0));
    print(eq9);
  end if;
end proc:</Text-field>
</Input>
</Group>
