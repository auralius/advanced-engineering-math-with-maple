<Group labelreference="L1" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">variation_of_parameters:=proc(pp,qq)
  local eq1, eq2, eq3, eq4, eq5, eq6,eq7;
  p(x):=pp;
  q(x):=qq;
  eq1:=diff(y(x),x)+p(x)*y(x)=q(x);

  printf(&quot;An ODE:&quot;);
  print(eq1);
  printf(&quot;Thus, we have:&quot;);
  print('p(x)'=p(x));
  print('q(x)'=q(x));

  printf(&quot;Step 1, find the homogenous solution:&quot;);
  y_h(x):=A*exp(-1*int(p(x),x)):
  print('y[h]=A*exp(-1*int(p(x),x))');
  print('y[h]'=y_h(x));

  printf(&quot;Step 2, find the complementary solution based on the homogenous solution by turning the A into a function A(x):&quot;);
  eq2:=y(x)=A(x)*exp(-1*int(p(x),x)):
  print('y(x)=A(x)*exp(-1*int(p(x),x))');
  print(eq2);

  printf(&quot;Step 3, find y'(x) from y(x):&quot;);
  eq3:=diff(eq2,x);
  print(eq3);

  printf(&quot;Step 4, to find A(x), substitute y(x) and y'(x) back to the ODE:&quot;);
  eq4:=subs([eq2, eq3],eq1);
  eq5:=solve(eq4,diff(A(x),x));
  print(eq1);
  print(eq4);
  print('diff(A(x),x)'=eq5);

  printf(&quot;Integrate both sides wrt x:&quot;);
  eq6:=A(x)=simplify(int(eq5,x)+C);
  print(eq6);

  printf(&quot;Step 5: substitute A(x) back to y(x) of the complementary solution:&quot;);
  eq7:=subs(eq6,eq2);
  print(eq2);
  print(simplify(eq7));
end proc:
</Text-field>
</Input>
</Group>
