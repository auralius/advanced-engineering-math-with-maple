<Group labelreference="L34" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Prob9_10_3 := proc(input)
  local u, e_hat, c, E;
  u:=input;
  e_hat[1]:=(1/sqrt(3))*&lt;1, 1, 0, -1&gt;;
  e_hat[2]:=(1/sqrt(3))*&lt;1, -1, -1, 0&gt;;
  e_hat[3]:=(1/sqrt(3))*&lt;1, 0, 1, 1&gt;;
  e_hat[4]:=(1/sqrt(3))*&lt;0, 1, -1, 1&gt;;

  c[1] := DotProduct(u, e_hat[1]);
  c[2] := DotProduct(u, e_hat[2]);
  c[3] := DotProduct(u, e_hat[3]);
  c[4] := DotProduct(u, e_hat[4]);

  printf(&quot;Given:&quot;);
  print(`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1]=e_hat[1], `#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2]=e_hat[2], 
        `#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3]=e_hat[3], `#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[4]=e_hat[4]);

  printf(&quot;Find the best approximation for:&quot;);
  print('u'=u);
  printf(&quot;The best approximation is given by:&quot;);

  print('`#mover(mi(&quot;u&quot;),mo(&quot;&amp;tilde;&quot;))` = c[1]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1] + c[2]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2] + 
        c[3]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3] + c[4]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[4]');

  printf(&quot;However, we are asked to do approximation one-by-one:&quot;);

  print('`#mover(mi(&quot;u&quot;),mo(&quot;&amp;tilde;&quot;))`[1] = c[1]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1]');
  print('`#mover(mi(&quot;u&quot;),mo(&quot;&amp;tilde;&quot;))`[2] = c[1]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1] + c[2]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2]');
  print('`#mover(mi(&quot;u&quot;),mo(&quot;&amp;tilde;&quot;))`[3] = c[1]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1] + c[2]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2] + 
        c[3]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3]');
  print('`#mover(mi(&quot;u&quot;),mo(&quot;&amp;tilde;&quot;))`[4] = c[1]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1] + c[2]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2] + 
        c[3]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3] + c[4]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[4]');

  printf(&quot;where:&quot;);
  print('c[1]' = 'u.`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1]', 'c[1]' = c[1]);
  print('c[2]' = 'u.`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2]', 'c[2]' = c[2]);
  print('c[3]' = 'u.`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3]', 'c[3]' = c[3]);
  print('c[4]' = 'u.`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[4]', 'c[4]' = c[4]);

  printf(&quot;The error is also calculated one-by-one:&quot;);
  E[1] := u-c[1]*e_hat[1];
  E[2] := u-c[1]*e_hat[1]-c[2]*e_hat[2];
  E[3] := u-c[1]*e_hat[1]-c[2]*e_hat[2]-c[3]*e_hat[3];
