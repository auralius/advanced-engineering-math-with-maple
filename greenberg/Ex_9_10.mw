<Group hide-input="false" labelreference="L21" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Prob9_10_2 := proc(input)
  local u, e_hat, c, E;
  u:=input;
  e_hat[1]:=(1/sqrt(5))*&lt;1, 0, 2, 0, 0&gt;;
  e_hat[2]:=(1/sqrt(6))*&lt;2, 0, -1, 0, 1&gt;;
  e_hat[3]:=&lt;0, 0, 0, 1, 0&gt;;

  c[1] := DotProduct(u, e_hat[1]);
  c[2] := DotProduct(u, e_hat[2]);
  c[3] := DotProduct(u, e_hat[3]);

  printf(&quot;Given:&quot;);
  print(`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1]=e_hat[1], `#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2]=e_hat[2], 
        `#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3]=e_hat[3]);

  printf(&quot;Find best approximation for:&quot;);
  print('u'=u);
  printf(&quot;We define the best approximation as:&quot;);
  print('`#mover(mi(&quot;u&quot;),mo(&quot;&amp;tilde;&quot;))` = c[1]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1] + c[2]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2] + 
        c[3]*`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3]');
  printf(&quot;where:&quot;);

  print('c[1]' = 'u.`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[1]', 'c[1]' = c[1]);
  print('c[2]' = 'u.`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[2]', 'c[2]' = c[2]);
  print('c[3]' = 'u.`#mover(mi(&quot;e&quot;),mo(&quot;&amp;circ;&quot;))`[3]', 'c[3]' = c[3]);

  <Font encoding="UTF-8">printf(&quot;The error is:\134n&quot;);</Font>
  E := u-c[1]*e_hat[1]-c[2]*e_hat[2]-c[3]*e_hat[3];
  print('E'='u- `#mover(mi(&quot;u&quot;),mo(&quot;&amp;tilde;&quot;))`');
  print('E'=simplify(E));
  print('norm(E)'=simplify(norm(E,2)));

end proc:</Text-field>
</Input>
</Group>
